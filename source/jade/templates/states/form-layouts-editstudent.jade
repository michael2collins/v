include ../mixins/formerr.jade
include ../mixins/formgen.jade

        
.row#form-layouts-editstudent(ng-controller='FormLayoutsControllerEditStudent as vmstudent')
    .col-lg-12
        uib-tabset(type='pills')
            uib-tab#tab-student(heading='Student Information')
                .row
                    div
                        .col-lg-12
                            .panel.panel-blue
                                .panel-heading Student
                                .panel-body.pan
                                  -var formnm="editstudent"
                                   form.horizontal-form(action='', novalidate, name='#{formnm}')
                                    .form-body.pal
                                      | <h3 class="block-heading">Student Information</h3>
                                      - var columns=2
                                      +formgen(stu_formObj,columns)
                                                        
                                      | <h3 class="block-heading">Address Information</h3>
                                      - var acolumns=2
                                      +formgen(addr_formObj,acolumns)
                                                                
                                      | <h3 class="block-heading">Dojo Information</h3>
                                      - var bcolumns=2
                                      +formgen(dojo_formObj,bcolumns)
                                                                
            tab#tab-student(heading='Class')
                .row
                    .col-lg-12
                        .portlet.box.portlet-orange
                            .portlet-header
                                .caption Media Gallery
                                .tools
                                    i.fa.fa-chevron-up
                                    i.fa.fa-cog(data-toggle="modal", data-target="#modal-config")
                                    i.fa.fa-refresh
                                    i.fa.fa-times
                            .portlet-body
                                .gallery-pages
                                    .row
                                        .btn-group(opt-kind, ok-key='filter')
                                            button.btn.btn-default.active(ok-sel='*') Show All
                                            button.btn.btn-default(ok-sel='.cat-{{category}}', ng-model='classcategories', ng-repeat='category in classcategories') {{category}}
                                        .mix-grid.ng-cloak(isotope-container, ng-cloak)
                                            .col-md-3(class='mix {{x.classcat}}', ng-repeat='x in xList', isotope-item)
                                                .hover-effect
                                                    .img
                                                        img.img-responsive(src='images/classes/{{x.classurl}}', alt='')
                                                    .info
                                                        h3 {{x.class}}
                                                        a.mix-link(href='#')
                                                            i.glyphicon.glyphicon-link
                                                        a.mix-zoom(href='images/classes/{{x.classurl}}')
                                                            i.glyphicon.glyphicon-search
 
            tab#tab-student(heading='Payment')
            tab#tab-student(heading='Student History')
