  body.topbar-fixed.sidebar-fixed(ng-app='ng-admin', ng-class="[header.header_topbar, header.boxed]")
    .default-page(ng-controller="AppController")

      //BEGIN TEMPLATE SETTING
      #temp-setting(ng-include="'templates/states/_includes/template-setting.html'", onload="loadtempsetting()")
      //END TEMPLATE SETTING

      //BEGIN TO TOP
      a#totop(href='#')
          i.fa.fa-angle-up
      //END BACK TO TOP

      #wrapper(ng-class="header.layout_menu")
          //BEGIN TOPBAR
          #topbar.navbar.navbar-default.navbar-static-top(role='navigation', ng-include="'templates/states/_includes/topbar.html'", style='margin-bottom: 0;', onload="loadTopbar()")
          //END TOPBAR

          //BEGIN SIDEBAR MENU
          #sidebar-wrapper(ng-include="'templates/states/_includes/sidebar.html'", ng-menu="", onload="loadSidebar()")
          //END SIDEBAR MENU

          //BEGIN PAGE WRAPPER
          #page-wrapper(ng-class="header.animation" , class="animated")

              //BEGIN PAGE HEADER & BREADCRUMB
              .page-header-breadcrumb(ng-include="'templates/states/_includes/header-breadcrumb.html'", onload="loadPageHeader()")
              //END PAGE HEADER & BREADCRUMB

              //BEGIN CONTENT
              .page-content(ng-view='')
              //END CONTENT

              //BEGIN FOOTER
              .page-footer
                  .copyright 2015 Â© V - Dojo Dashboard.
              //END FOOTER
          //END PAGE WRAPPER
    .extra-page(hide)
