{
    "fonts.fontSize": "12px",
    "fonts.fontFamily": "'SourceCodePro-Medium', ＭＳ ゴシック, 'MS Gothic', monospace",
    "me.drewh.jsbeautify.on_save": false,
    "dnbard.documents-toolbar.icons": true,
    "dnbard.documents-toolbar.smallIcons": false,
    "dnbard.documents-toolbar.tooltip": false,
    "dnbard.documents-toolbar.close_left": false,
    "dnbard.documents-toolbar.workingFiles": true,
    "dnbard.documents-toolbar.brackets_font": true,
    "dnbard.documents-toolbar.grayscaleIcons": false,
    "dnbard.documents-toolbar.showCloseButton": true,
    "brackets-git.gitPath": "C:\\Program Files (x86)\\Git\\cmd\\git.exe",
    "brackets-git.stripWhitespaceFromCommits": true,
    "brackets-git.addEndlineToTheEndOfFile": true,
    "brackets-git.removeByteOrderMark": false,
    "brackets-git.normalizeLineEndings": false,
    "brackets-git.useGitGutter": true,
    "brackets-git.markModifiedInTree": true,
    "brackets-git.showBashButton": true,
    "brackets-git.showReportBugButton": true,
    "brackets-git.useCodeInspection": true,
    "brackets-git.useGitFtp": false,
    "brackets-git.useVerboseDiff": false,
    "brackets-git.useDifftool": false,
    "brackets-git.showTerminalIcon": false,
    "brackets-git.clearWhitespaceOnSave": false,
    "brackets-git.avatarType": "AVATAR_COLOR",
    "brackets-git.dateMode": 1,
    "brackets-git.dateFormat": null,
    "brackets-git.enableAdvancedFeatures": false,
    "brackets-git.panelShortcut": "Ctrl-Alt-G",
    "brackets-git.commitCurrentShortcut": null,
    "brackets-git.commitAllShortcut": null,
    "brackets-git.bashShortcut": null,
    "brackets-git.pushShortcut": null,
    "brackets-git.pullShortcut": null,
    "brackets-git.gotoPrevChangeShortcut": null,
    "brackets-git.gotoNextChangeShortcut": null,
    "brackets-git.refreshShortcut": null,
    "brackets-git.gitTimeout": 30,
    "brackets-git.terminalCommand": null,
    "brackets-git.terminalCommandArgs": null,
    "brackets-git.debugMode": false,
    "brackets-editorconfig.enabled": true,
    "jwolfe.file-tree-exclude.list": [
        "node_modules",
        "bower_components",
        ".git",
        "dist",
        "vendor"
    ],
    "com.github.dsbonev.WhitespaceNormalizer.enabled": "true",
    "com.github.dsbonev.WhitespaceNormalizer.trim": "true",
    "com.github.dsbonev.WhitespaceNormalizer.transf": "true",
    "com.github.dsbonev.WhitespaceNormalizer.eofnl": "true",
    "angular-snippets.enabled": true,
    "themes.theme": "delkos",
    "brackets-indent-guides.enabled": true,
    "brackets-icons.iconset": "devicons",
    "linting.collapsed": true,
    "linting.JSLint.collapsed": true,
    "denniskehrig.ShowWhitespace.checked": false,
    "language": {
        "javascript": {
            "linting.prefer": [
                "JSHint",
                "JSCS"
            ],
            "linting.usePreferredOnly": true
        }
    },
    "php-sig.php-smarthints.filteredFunctionList": [
        "mysql",
        "pdo"
    ]
}